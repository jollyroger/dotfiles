! Xft settings
Xft.dpi: 96.0
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.antialias: true
Xft.rgba: none

! Basic X terminal emulator settings
!*color0:	#000000
!*color1:	#9e1828
!*color2:	#aece92
!*color3:	#968a38
!*color4:	#414171
!*color5:	#963c59
!*color6:	#418179
!*color7:	#bebebe
!*color8:	#666666
!*color9:	#cf6171
!*color10:	#c5f779
!*color11:	#fff796
!*color12:	#4186be
!*color13:	#cf9ebe
!*color14:	#71bebe
!*color15:	#ffffff

! Default font for any basic X client
*Font:		-misc-fixed-medium-r-semicondensed-*-13-*-*-*-*-*-iso10646-1

! URxvt settings
URxvt.font:	-misc-fixed-medium-r-semicondensed-*-13-*-*-*-*-*-iso10646-1
!URxvt*iconFile: /usr/share/pixmaps/urxvt_32x32.xpm
URxvt.background: #000000
URxvt.foreground: #AFB8FA
URxvt.fading: 10  
URxvt.shading: 0
URxvt.transparent: false
URxvt.fadeColor: #666666
URxvt.tintColor: #3F3F3F
URxvt.scrollstyle: plain
URxvt.scrollColor: #C9BD9A
URxvt.troughColor: #000000
URxvt.scrollBar: true
URxvt.scrollBar_right: false
URxvt.scrollBar_floating: false
URxvt.cursorBlink: true
URxvt.internalBorder: 0
URxvt.externalBorder: 0
URxvt.borderLess: false
URxvt.insecure: true
URxvt.urgentOnBell: true
!URxvt.perl-ext-common: default,tabbed,matcher

! Xterm keys simulation
URxvt.keysym.C-Prior: \033[5;5~
URxvt.keysym.C-Next: \033[6;5~
URxvt.keysym.C-Left: \033[1;5D
URxvt.keysym.C-Right: \033[1;5C
URxvt.keysym.C-Up: \033[1;5A
URxvt.keysym.C-Down: \033[1;5B

! Tkabber settings
*mainRosterWidth: 200
